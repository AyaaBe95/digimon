<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../layout/head') %>

</head>

<body>
  <section class="info">
    <% data.forEach(element => { %>
      <div>
        <p> Name: <%= element.name %> </p>
        <img src="<%= element.img %>">
        <p> Level: <%= element.level %> </p>

        <form action="/details/<%= element.id %> " method="GET">
          <input type="submit" value="View Details">
        </form>

      </div>
     
    <% }) %>

  </section>
  <%- include('../layout/footer') %>


</body>

</html>