<!DOCTYPE html>
<html lang="en">

<head>
  <%- include('../layout/head') %>

</head>

<body>
  <section class="info">
    <div>
      <p>Name: <%= data.name %> </p>
      <img src="<%= data.img %> ">
      <p>Level: <%= data.level %> </p>
    </div>
    <div>
      <form action="/update/<%= data.id %>?_method=put" method="POST">
        <input type="text" name="name" value="<%= data.name %> ">
        <br>
        <input type="text" name="img" value="<%= data.img%>">
        <br>
        <input type="text" name="level" value="<%= data.level%>">
        <br>
        <input type="submit" value="UPDATE">
        

      </form>
      <form action="/delete/<%= data.id %>?_method=delete" method="POST">
        <input type="submit" value="Delete">

      </form>

    </div>
     

  </section>
  <%- include('../layout/footer') %>

</body>

</html>